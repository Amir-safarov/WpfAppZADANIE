<Page x:Class="WpfAppZADANIE.Pages.Reg"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfAppZADANIE.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Reg">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <TextBlock>EMail</TextBlock>
                <TextBox Name="EmailTB"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock>Phone</TextBlock>
                <TextBox Name="PhoneTB"></TextBox>
            </StackPanel>
            <Button Name="ZaxodBTN" Click="ZaxodBTN_Click">🍌</Button>
        </StackPanel>
    </Grid>
</Page>
